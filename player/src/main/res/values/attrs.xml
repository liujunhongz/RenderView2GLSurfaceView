<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ENDownloadView">
        <attr name="line_color" format="color"/>
        <attr name="bg_line_color" format="color"/>
        <attr name="text_color" format="color"/>
        <attr name="line_width" format="integer"/>
        <attr name="bg_line_width" format="integer"/>
        <attr name="text_size" format="integer"/>
    </declare-styleable>
    <declare-styleable name="RatioLayout">
        <attr name="ratioEnabled" format="boolean"/>
        <attr name="ratioWidth" format="float"/>
        <attr name="ratioHeight" format="float"/>
        <attr name="reference" format="enum">
            <enum name="width" value="0"/>
            <enum name="height" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="StandardPlayer">
        <attr name="player" format="enum">
            <enum name="alivc" value="1"/>
            <enum name="ijk" value="2"/>
        </attr>
    </declare-styleable>
</resources>